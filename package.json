{
  "name": "lua-state",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "license": "MIT",
  "author": "quaternion",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./js/index.js",
      "import": "./js/index.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "gypfile": false,
  "preferUnplugged": true,
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:quaternion/node-lua-state.git"
  },
  "files": [
    "build-tools",
    "js",
    "scripts",
    "src",
    "types",
    "binding.gyp"
  ],
  "binary": {
    "module_name": "node-lua-state",
    "module_path": "prebuilds/{platform}-{arch}/",
    "napi_versions": [
      8
    ]
  },
  "bin": {
    "lua-state": "./bin/lua-state.js"
  },
  "scripts": {
    "install": "node scripts/install.js",
    "pack:linux": "tar -czf lua-state-v1.0.0-napi-v8-linux-x64.tar.gz package.json prebuilds/linux-x64/",
    "test": "node --expose-gc --test tests/**/*.test.js"
  },
  "dependencies": {
    "detect-libc": "^2.1.1"
  },
  "optionalDependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp": "^11.4.2"
  },
  "peerDependencies": {
    "tar": "7.x"
  }
}
